<h1>Groups</h1>

<div id="groups">
  <% @groups.each do |group| %>
    <%= render group %>
    <p>
      <%= link_to "Show this group", group %>
    </p>
  <% end %>
</div>
<%= button_to(
        "Log Out",
        destroy_user_session_path,
        method: :delete
      ) %>

<%= link_to "New group", new_group_path %>
